#!/bin/bash

readonly file_systemlog_ap=/tmp/.systemlog_ap
readonly file_url_conf_tmp=/tmp/urllist.conf
readonly file_url_conf=/etc/um/urllist.conf
readonly file_register_data=/data/.register.json
readonly file_register_tmp=/tmp/.register.json

get_register_json_file() {
	local file_data=${file_register_data}
	local file_tmp=${file_register_tmp}

	local mac=$(cat ${file_data} 2> /dev/null| jq -j '.mac|strings')
	if [[ "${mac}" ]]; then
		echo ${file_data}
	else
		echo ${file_tmp}
	fi
}
readonly FILE_REGISTER=$(get_register_json_file)