{
	"cloud": {
		"atbus": {
			"protocol": "https",
			"domain": "119.29.7.231",
			"port": "22222",
			"base": "LMS/lte",
			"user": "autelanauteviewlms",
			"pass": "autelanauteviewlms20140925",
			"cert": "/etc/platform/conf/server.cert",
			"register": "/data/.register.json",

			"__must_exist__":"register"
		}
	},

	"action": {
		"register": {
			"cloud": "atbus",
			"url": "lteRegister.do",
			"cookie": "/tmp/plt/.platform2.register.cookie",
			"output": "/tmp/plt/.platform2.register.output",
			"error": "/tmp/plt/.platform2.register.error",
			"contenttype": "application/json",
			"interval": "30",
			"timeout": "30"
		},

		"command": {
			"cloud": "atbus",
			"url": "lteCommand.do",
			"cookie": "/tmp/plt/.platform2.command.cookie",
			"output": "/tmp/plt/.platform2.command.output",
			"error": "/tmp/plt/.platform2.command.error",
			"contenttype": "application/json",
			"cmdlast": "/tmp/plt/.platform2.command.last",
			"cmdlist": "/tmp/plt/.platform2.command.list",
			"timeout": "30"
		},

		"keepalive": {
			"cloud": "atbus",
			"url": "lteHeartBeat.do",
			"cookie": "/tmp/plt/.platform2.keepalive.cookie",
			"output": "/tmp/plt/.platform2.keepalive.output",
			"error": "/tmp/plt/.platform2.keepalive.error",
			"contenttype": "application/json",
			"timeout": "30"
		},

		"ulog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform2.ulog.output",
			"timeout": "120"
		},

		"klog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform2.klog.output",
			"timeout": "120"
		},

		"ilog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform2.ilog.output",
			"timeout": "120"
		}
	}
}
