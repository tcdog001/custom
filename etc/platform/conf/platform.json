{
	"cloud": {
		"atbus": {
			"protocol": "https",
			"domain": "atbus.autelan.com",
			"port": "8443",
			"base": "LMS/lte",
			"user": "autelanauteviewlms",
			"pass": "autelanauteviewlms20140925",
			"cert": "/etc/platform/conf/server.cert",
			"register": "/data/.register.json",

			"__must_exist__":"register"
		},

		"update1": {
			"protocol": "http",
			"domain": "update1.9797168.com",
			"port": "821",
			"base": "wifibox"
		}
	},

	"action": {
		"register": {
			"cloud": "atbus",
			"url": "lteRegister.do",
			"cookie": "/tmp/plt/.platform.register.cookie",
			"output": "/tmp/plt/.platform.register.output",
			"error": "/tmp/plt/.platform.register.error",
			"contenttype": "application/json",
			"interval": "30",
			"timeout": "30"
		},

		"command": {
			"cloud": "atbus",
			"url": "lteCommand.do",
			"cookie": "/tmp/plt/.platform.command.cookie",
			"output": "/tmp/plt/.platform.command.output",
			"error": "/tmp/plt/.platform.command.error",
			"contenttype": "application/json",
			"cmdlast": "/tmp/plt/.platform.command.last",
			"cmdlist": "/tmp/plt/.platform.command.list",
			"timeout": "30"
		},

		"keepalive": {
			"cloud": "atbus",
			"url": "lteHeartBeat.do",
			"cookie": "/tmp/plt/.platform.keepalive.cookie",
			"output": "/tmp/plt/.platform.keepalive.output",
			"error": "/tmp/plt/.platform.keepalive.error",
			"contenttype": "application/json",
			"timeout": "30"
		},

		"ulog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform.ulog.output",
			"timeout": "120"
		},

		"klog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform.klog.output",
			"timeout": "120"
		},

		"ilog": {
			"cloud": "atbus",
			"output": "/tmp/plt/.platform.ilog.output",
			"timeout": "120"
		},

		"gpslog": {
			"cloud": "update1",
			"output": "/tmp/plt/.platform.gpslog.output",
			"error": "/tmp/plt/.platform.gpslog.error",
			"signature": "/etc/platform/conf/gpslog.key",
			"form": {
				"type": "gps",
				"ident": "get_json_mac",
				"content": "string"
			},
			"prefix":"gps",
			"timeout": "60",

			"__must_exist__":"signature"
		},

		"syslog": {
			"cloud": "update1",
			"output": "/tmp/plt/.platform.syslog.output",
			"error": "/tmp/plt/.platform.syslog.error",
			"signature": "/etc/platform/conf/syslog.key",
			"form": {
				"type": "sys",
				"ident": "get_mac",
				"content": "file"
			},
			"prefix":"sys",
			"timeout": "120",

			"__must_exist__":"signature"
		}
	}
}
